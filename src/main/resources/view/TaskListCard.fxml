<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<GridPane HBox.hgrow="ALWAYS">
			<columnConstraints>
				<ColumnConstraints hgrow="NEVER" maxWidth="80.0" minWidth="30.0" prefWidth="80.0" />
				<ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="450.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="57.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="187.0" minWidth="10.0" prefWidth="143.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label fx:id="id" styleClass="cell_big_label" text="\$id" />
				<Label fx:id="name" minWidth="-Infinity" prefWidth="450.0" styleClass="cell_big_label" text="\$name" GridPane.columnIndex="1" />
				<Label fx:id="tags" styleClass="cell_small_label" text="\$tags" GridPane.columnIndex="2" />
				<Label fx:id="details" minWidth="-Infinity" prefWidth="300.0" styleClass="cell_big_label" text="\$details" textAlignment="RIGHT" GridPane.columnIndex="3" GridPane.halignment="RIGHT" />
			</children>
		</GridPane>
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</HBox>
